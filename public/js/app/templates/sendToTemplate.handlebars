<select id="sendToSelect" multiple="multiple" class="p-sendto-select">
  {{#if view.showMyFeedOption}}
    <option selected="selected" value="{{controller.session.currentUser.username}}" class="p-sendto-selected">My feed</option>
  {{else}}
    <option value="{{controller.session.currentUser.username}}" class="p-sendto-option">My feed</option>
  {{/if}}
  {{#each controller.session.currentUser.feeds as |feed|}}
    <option value="{{feed.user.username}}" class="p-sendto-option">{{feed.user.screenName}} ({{feed.user.username}})</option>
  {{/each}}
</select>

{{#if view.showToggler}}
  <span class="send-to">
    <a {{action 'toggleEditability' target=view}} class="p-sendto-toggler">Add/Edit</a>
  </span>
{{/if}}
